{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "allowedIps": {
      "value": "${AZURE_ALLOWED_IPS}"
    },
    "assignRoles": {
      "value": "${ASSIGN_ROLES=false}"
    },
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "workloadName": {
      "value": "documentocr"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "runningOnAdo": {
      "value": "${TF_BUILD}"
    },
    "runningOnGh": {
      "value": "${GITHUB_ACTIONS}"
    },
    "tenantId": {
      "value": "${AZURE_TENANT_ID}"
    },
    "webAppClientId": {
      "value": "${AZURE_WEB_APP_CLIENT_ID}"
    },
    "azureAdDomain": {
      "value": "${AZURE_AD_DOMAIN}"
    },
    "tags": {
      "value": "${AZURE_TAGS}"
    }
  }
}
