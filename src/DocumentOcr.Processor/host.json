{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request;Exception"
            },
            "enableLiveMetricsFilters": true
        },
        "logging": {
            "fileLoggingMode": "debugOnly",
            "logLevel": {
                "default": "Warning",
                "Host.Aggregator": "Trace",
                "Host.Results": "Information",
                "Function": "Information"
            }
        }
    }
}